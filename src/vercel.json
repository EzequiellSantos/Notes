{
    "version": 2,
    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/notes",
        "dest": "routes/notes.js"
      },
      {
        "src": "/src/(.*)",
        "dest": "index.js"
      }
    ]
  }
  